{
  "name": "@maxmilton/solid-router",
  "version": "0.3.2",
  "type": "module",
  "description": "A lightweight History API based router for Solid",
  "repository": "maxmilton/solid-router",
  "author": "Max Milton <max@happycode.ltd>",
  "license": "MIT",
  "main": "dist/index.jsx",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm run prebuild && tsc",
    "dev": "tsc --watch --preserveWatchOutput",
    "lint": "pnpm run lint:js && pnpm run lint:ts",
    "lint:js": "eslint --ignore-path .gitignore --ext .cjs,.js,.jsx,.mjs,.ts,.tsx .",
    "lint:ts": "tsc --noEmit --project test",
    "prebuild": "rm -rf dist; mkdir dist",
    "pretest-e2e": "pnpm run --dir test/fixtures build",
    "test": "jest --coverage",
    "test-e2e": "pnpm run pretest-e2e && jest --testRegex='test/e2e/index.ts$' test/e2e/index.ts"
  },
  "peerDependencies": {
    "solid-js": "^1.5.0"
  },
  "dependencies": {
    "qss": "^2.0.3",
    "regexparam": "^2.0.1"
  },
  "devDependencies": {
    "@babel/core": "7.20.12",
    "@babel/plugin-transform-modules-commonjs": "7.20.11",
    "@babel/preset-typescript": "7.18.6",
    "@jest/types": "29.4.1",
    "@rollup/plugin-babel": "6.0.3",
    "@rollup/plugin-node-resolve": "15.0.1",
    "@types/babel__core": "7.20.0",
    "@types/jest": "29.4.0",
    "@types/node": "18.11.18",
    "@typescript-eslint/eslint-plugin": "5.50.0",
    "@typescript-eslint/parser": "5.50.0",
    "babel-preset-solid": "1.6.9",
    "eslint": "8.32.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jest": "27.2.1",
    "eslint-plugin-security": "1.6.0",
    "eslint-plugin-unicorn": "45.0.2",
    "get-port": "6.1.2",
    "jest": "29.4.1",
    "jest-environment-jsdom": "29.4.1",
    "jest-resolve": "29.4.1",
    "kleur": "4.1.5",
    "playwright-chromium": "1.29.2",
    "prettier": "2.8.3",
    "prettier-plugin-pkg": "0.17.1",
    "prettier-plugin-sh": "0.12.8",
    "rollup": "3.11.0",
    "sirv": "2.0.2",
    "solid-js": "1.6.9",
    "solid-testing-library": "0.5.0",
    "typescript": "4.9.5"
  }
}
