{
  "name": "@maxmilton/solid-router",
  "version": "0.3.1",
  "description": "A lightweight History API based router for Solid",
  "repository": "maxmilton/solid-router",
  "author": "Max Milton <max@happycode.ltd>",
  "license": "MIT",
  "main": "dist/index.jsx",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm run prebuild && tsc",
    "dev": "tsc --watch --preserveWatchOutput",
    "lint": "pnpm run lint:js && pnpm run lint:ts",
    "lint:js": "eslint --ignore-path .gitignore --ext .cjs,.js,.jsx,.mjs,.ts,.tsx .",
    "lint:ts": "tsc --noEmit --project test",
    "prebuild": "rm -rf dist; mkdir dist",
    "test": "jest --coverage",
    "test-build-fixtures": "pnpm run --dir test/fixtures build",
    "test-e2e": "pnpm run test-build-fixtures && jest --testRegex='test/e2e/index.ts$' test/e2e/index.ts"
  },
  "peerDependencies": {
    "solid-js": "^1.1.0"
  },
  "dependencies": {
    "qss": "^2.0.3",
    "regexparam": "^2.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.17.5",
    "@babel/plugin-transform-modules-commonjs": "7.16.8",
    "@babel/preset-typescript": "7.16.7",
    "@rollup/plugin-babel": "5.3.1",
    "@rollup/plugin-node-resolve": "13.1.3",
    "@testing-library/jest-dom": "5.16.2",
    "@types/jest": "27.4.1",
    "@types/node": "17.0.17",
    "@typescript-eslint/eslint-plugin": "5.13.0",
    "@typescript-eslint/parser": "5.13.0",
    "babel-preset-solid": "1.3.6",
    "eslint": "8.11.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "16.1.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-jest": "26.1.1",
    "eslint-plugin-unicorn": "41.0.0",
    "get-port": "6.1.2",
    "jest": "27.5.1",
    "jest-resolve": "27.5.1",
    "kleur": "4.1.4",
    "playwright-chromium": "1.20.1",
    "prettier": "2.5.1",
    "prettier-plugin-pkg": "0.11.1",
    "prettier-plugin-sh": "0.8.1",
    "rollup": "2.70.1",
    "sirv": "2.0.2",
    "solid-js": "1.3.10",
    "solid-testing-library": "0.3.0",
    "typescript": "4.6.2"
  }
}
